{
  "extends": [
    "eslint:recommended",
    "plugin:import/recommended",
    "plugin:import/typescript"
  ],
  "rules": {
    "quotes": ["error", "double"],
    "semi": ["error", "always"],
    "indent": ["error", 2],
    "import/no-unresolved": [2, {"commonjs": true, "amd": true}],
    "import/newline-after-import": ["error", { "count": 2 }]
  }
}
